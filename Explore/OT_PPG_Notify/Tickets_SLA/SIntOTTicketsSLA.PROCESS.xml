                                                <title>AssignFilterResulset</title>
                                                <operation source="formula" to="temp.vTempFilterResulset">
                                                   <expression language="XQuery">let $nRootWorkItems := &lt;Root&gt;{$temp.vTempResultset}&lt;/Root&gt;

let $tZone := fn:implicit-timezone()

let $l_minor := util:toDecimal($temp.vTempMinor,2)
let $l_major := util:toDecimal($temp.vTempMajor,2)

let $l_filterResultset := &lt;WorkItems&gt;
                          {
						  
                          for $workItem in $nRootWorkItems/*:WorkItem[*:type=$temp.vTempType ]
                          let $edaTZone := fn:adjust-dateTime-to-timezone(xs:dateTime($workItem/*:custom_eda/text()),xs:dayTimeDuration($tZone))
                          let $createdDateTZone := fn:adjust-dateTime-to-timezone(xs:dateTime($workItem/*:created_date/text()),xs:dayTimeDuration($tZone))
                          let $sysdateTZone := fn:adjust-dateTime-to-timezone(fn:current-dateTime(),xs:dayTimeDuration($tZone))
						  let $edaNull := date:addToDate($createdDateTZone, 'HH24', 1)
                          let $l_sla := if($workItem/*:custom_eda/text()) then (
                                    	date:dateDiff($edaTZone , $createdDateTZone ,'MI')
                                        ) else ( date:dateDiff($edaNull , $createdDateTZone ,'MI') )
                                         
                          let $l_time := date:dateDiff($sysdateTZone , $createdDateTZone ,'MI')
                          let $l_calculate := $l_sla - $l_time
                          where 1 = 1
						  and ($l_calculate ge $l_minor  and $l_calculate le $l_major )
                          return
                            &lt;WorkItem&gt;
                              &lt;id&gt;{$workItem/*:id/text()}&lt;/id&gt;
                              &lt;created_date&gt;{$workItem/*:created_date/text()}&lt;/created_date&gt;
                              &lt;custom_eda&gt;{if($workItem/*:custom_eda/text()) then (
                                         $workItem/*:custom_eda/text()
                                                     ) else (
                                    	     $edaNull
                                        )}&lt;/custom_eda&gt;
                              &lt;custom_edc&gt;{$workItem/*:custom_edc/text()}&lt;/custom_edc&gt;
                              &lt;timeRemainingSLA&gt;{fn:concat(fn:round($l_calculate,2), 'Min~')}&lt;/timeRemainingSLA&gt;
                            &lt;/WorkItem&gt;
                          }
                          &lt;/WorkItems&gt;
                        
return $l_filterResultset

</expression>
                                                </operation>
